#include <iostream>
using namespace std;

void Hasil(int a[][3], int setir, int sutun) {
    int hasil = 1; 
    for (int i = 0; i < setir; i++)
        for (int j = 0; j < sutun; j++)
            hasil *= a[i][j];

    cout << "Hasil = " << hasil << endl;
}

int main() {
    int a[2][3] = {
        {4, 8, 3},
        {6, 10, 1}
    };

    Hasil(a, 2, 3);

    return 0;
}
