#include <iostream>
using namespace std;

void Max(int a[], int n) {
    int max = a[0];
    for (int i = 1; i < n; i++)
        if (a[i] > max)
            max = a[i];
            cout << "Max = " << max << endl;
}
int main() {
    int a[] = {4, 8, 3, 6, 10};
    int n = 5;
    Max(a, n);

    return 0;
}
