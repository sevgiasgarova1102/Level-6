#include <iostream>
using namespace std;

void Cem(int a[][3], int setir, int sutun) {
    int cem = 0;

    for (int i = 0; i < setir; i++)
        for (int j = 0; j < sutun; j++)
            cem += a[i][j];

    cout << "Cem = " << cem << endl;
}

int main() {
    int a[2][3] = {
        {4, 8, 3},
        {6, 10, 1}
    };

    Cem(a, 2, 3);

    return 0;
}
